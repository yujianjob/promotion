function UrlEncode(code) {
    var EnCode = "";
    var obj = JSON.stringify({ Code: code });
    $.ajax({
        cache: false,
        async: false,
        type: 'post',
        contentType: 'application/json',
        url: '/LearnOnLine/EnCode',
        data: obj,
        dataType: 'json',
        success: function (data) {
            if (!data || !data.Data || data.Data.length == 0) {
                return;
            }

            EnCode = data.Data;
        },
        error: function () {
        }
    });

    return EnCode;
}

function Trim(str) {
    return str.replace(/(^\s*)|(\s*$)/g, "");
}