function search() {
    var url = $('#url').val();

    if (checkts($('#searchTitle').val())){
        window.location.href = url + "&searchTitle=" + $('#searchTitle').val() + "&organId=" + $('#organId').val();
    }
}

function checkts(search)
{
    var reg = new RegExp('^[^@\/\\#$%&\^\*\<\>]+$');
    if (!reg.test(search)) {
        Alert('搜索的内容不能输入特殊字符 ^ @ / \ # $ % & * < >');
        return false;
    }
}