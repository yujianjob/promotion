//切换班级
function change(id, type) {
    $('#showlist').attr("src", "CoursemarketTCList?Id=" + id + "&Type=" + type);
}

//自适应高度
function SetWinHeight2(height) {
    iframeObj = $('#showlist')[0];
    if (navigator.userAgent.indexOf('MSIE') > 0) {
        height += 50;
    }

    if (iframeObj != null) {
        iframeObj.height = height;
    }
}